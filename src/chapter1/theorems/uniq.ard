\import src.chapter1.definitions.def

\func C_xx {A B : \Type} : A xx B -> \Type
  => \lam (u : A xx B) => ((u.1, u.2) = u)

\func uniq_xx {A B : \Type} :
  \Pi(w : A xx B) -> ((w.1, w.2) = w)
  => indAxB(C_xx)(\lam (a : A) (b : B) => refl(A xx B)(a,b))

\func uniq_One : \Pi(w : One) -> (w = One.star)
  => indOne(\lam x => (x = One.star))(refl(One)(One.star))